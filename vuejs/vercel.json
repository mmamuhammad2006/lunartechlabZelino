{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://your-api-endpoint.com"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/index.html"
      }
    ]
}