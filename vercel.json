{
    "version": 2,
    "builds": [
      {
        "src": "vuejs/package.json",
        "use": "@vercel/vite"  
      },
      {
        "src": "server/api/index.js", 
        "use": "@vercel/node"    
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/api/index.js" 
      },
      {
        "src": "/(.*)",
        "dest": "/vuejs/dist/$1"
      }
    ]
  }
  