{
    "version": 2,
    "builds": [
      {
        "src": "server/api/index.js",
        "use": "@vercel/node" 
      },
      {
        "src": "vuejs/dist/**/*",
        "use": "@vercel/static" 
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "server/api/index.js" 
      },
      {
        "src": "/(.*)", 
        "dest": "/dist/index.html"  
      }
    ]
  }